
class Voting_system {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        int age = sc.nextInt();
       // public void age () throws MyException {
            if (age < 18 | age > 60) {
                System.out.println("not allowed for voting");
            } else {
                System.out.println("allow for voting");
            }

        }
    }
